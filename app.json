{
  "expo": {
    "name": "Wintinkin-Movies",
    "slug": "Wintinkin-Movies",
    "version": "1.0.0",
    "orientation": "default",
    "primaryColor": "#2F80ED",
    "icon": "./assets/icon.png",
    "githubUrl": "https://github.com/uwemneku/Book-store-React-Native",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
